    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Currency Feed</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    @RenderBody()
    @Scripts.Render("~/bundles/jquery")
